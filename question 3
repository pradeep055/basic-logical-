# print the repeating element in array

<script>
    function printRepeating(arr,size)
    { 
        var i,j;
        document.writye("repeated element is : ");
        for(i=0;i<=size;i++)
        { 
            for(j=i+1; j<size; j++)
            { 
             if (arr[i]== arr[j])
                 document.write(arr[i] + " ");
            }      
         } 
     }    
var arr =[1,2,3,4......n];
var arr_size=arr.length;
printRepeating(arr,arr_size);
</script>
